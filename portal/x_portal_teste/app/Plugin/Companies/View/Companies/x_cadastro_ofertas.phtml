<script>
    function readURL(input) {

        if (input.files && input.files[0]) {
            var reader = new FileReader();

            reader.onload = function(e) {
                $('#blah').attr('src', e.target.result);
            }

            reader.readAsDataURL(input.files[0]);
        }
    }
    $(function() {
        $("#uper").change(function() {
            readURL(this);
        });

        $("#teste").click(function() {
            $("#cont-meu-publico").fadeOut(0);
            $("#cont-cad-oferta").fadeIn(1500);
        });

        $("#btn-my-public").click(function() {
            $("#cont-cad-oferta").fadeOut(0);
            $("#cont-meu-publico").fadeIn(1500);
        });

        $("#btn-img-error").click(function() {
            $(".valida-error").toggle(400);
        });

        /**
         * Calcula desconto
         * @returns {undefined}
         */
        $("#novo-preco").keyup(function() {
            var original = $("#original-preco").val().replace(".", "").replace(",", ".");
